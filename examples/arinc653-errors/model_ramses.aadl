package model_ramses
public
	with arinchm;
	with RAMSES_Properties;
	
	system node extends arinchm::node
	end node;

	system implementation node.ramses extends arinchm::node.impl
	properties
		Scheduling_Protocol => (ARINC653) applies to cpu;
		Scheduling_Protocol => (POSIX_1003_HIGHEST_PRIORITY_FIRST_PROTOCOL) applies to cpu.partition_secure,cpu.partition_common;
		Period => 500ms applies to cpu.partition_secure,cpu.partition_common;
		RAMSES_Properties::Target => "pok" applies to cpu;
	end node.ramses;
	
end model_ramses;
